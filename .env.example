# Bot token (Discord Developer Portal'dan alınır)
BOT_TOKEN=token_buraya_gelecek

# Bot client ID (Discord Developer Portal'dan alınır)
CLIENT_ID=client_id_buraya_gelecek

# Guild ID (Sunucu ID'si - Discord'dan kopyalanabilir)
GUILD_ID=sunucu_id_buraya_gelecek

# Tag role ID (Taglı kullanıcılara verilecek rol ID'si)
TAG_ROLE_ID=rol_id_buraya_gelecek

# Hedef Tag (Taglı kullanıcıların alacağı tag)
TARGET_TAG=OHİO

# Log kanalı ID'si (Bot loglarının gönderileceği kanal)
LOG_CHANNEL_ID=log_kanal_id_buraya_gelecek

# Prefix komutları için prefix (varsayılan: !)
PREFIX=!
